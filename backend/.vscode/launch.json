{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Iniciar o Chrome em relação a localhost",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Executar Script: start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}\\backend"
    }
  ]
}
